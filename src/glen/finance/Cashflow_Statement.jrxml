	<queryString language="SQL">
		<![CDATA[SELECT
     fin_cashflow.`cashflow_id` AS ID,
     fin_cashflow.`cashflow_date` AS Date,
     fin_cashflow.`cashflow_description` AS Description,
     fin_cashflow.`cashflow_method` AS Method,
     fin_cashflow.`cashflow_amount` AS Amount,
     fin_cashflow.`cashflow_payment_type` AS Type,
     fin_cashflow.`cashflow_payment_status` AS Status,
     fin_cashflow.`cashflow_department` AS Department,
     (SELECT SUM(cashflow_amount) FROM `fin_cashflow` fin_cashflow WHERE cashflow_date >= '$P!{firstdate}' AND cashflow_date < '$P!{seconddate}' AND cashflow_payment_status='Credit') AS CreditAmt,
     (SELECT SUM(cashflow_amount) FROM `fin_cashflow` fin_cashflow WHERE cashflow_date >= '$P!{firstdate}' AND cashflow_date < '$P!{seconddate}' AND cashflow_payment_status='Debit') AS DebitAmt,
     ((SELECT SUM(cashflow_amount) FROM `fin_cashflow` fin_cashflow WHERE cashflow_date >= '$P!{firstdate}' AND cashflow_date < '$P!{seconddate}' AND cashflow_payment_status='Credit')-(SELECT SUM(cashflow_amount) FROM `fin_cashflow` fin_cashflow WHERE cashflow_date >= '$P!{firstdate}' AND cashflow_date < '$P!{seconddate}' AND cashflow_payment_status='Debit')) AS Balance

FROM
     `fin_cashflow` fin_cashflow
WHERE
     cashflow_date >= '$P!{firstdate}' AND cashflow_date < '$P!{seconddate}'
ORDER BY
     cashflow_payment_status, cashflow_department]]>
	</queryString>
